  abstract = {Biochemical methods are the basis for generating curated interactomic data. The time and resources required to implement protein-protein interaction protocols limit the amount of data available to databases. The overrepresentation of model organisms also reduces the heterogeneity of data sets. Consumers must rely on alternative means for obtaining the numerous data points required for robust statistical analyses. This investigation presents an in silico approach to generate de novo co-expression networks using high-throughput screening results as substrate. The investigation developed separate implementations of the algorithm for the TORQUE cluster and CRAY XMT. Benchmark results are available for the initial phase of the algorithm for both systems. Further investigation is required to fully optimize the final phase for the CRAY XMT. Nevertheless, the TORQUE cluster produced results using the complete algorithm. Post-processing revealed proteins critical to quorum-sensing in Pseudomonas aeroginosa biofilms. The algorithm generated suitable co-expression networks using a conventional Affymetrix microarray data to facilitate countermeasure development against an infectious agent.},
  annotation = {note: Center for Biotechnology Education 7th Annual Research Symposium, The Johns Hopkins University, Montgomery County Campus Building 3, Rockville, MD},
  file = {/Users/dnanto/Zotero/storage/77K39CLH/Negrón - 2012 - Co-Expression Network Generation from Microarray D.pptx}
}

@misc{negronColorimetricPositionalApplication2024,
  type = {Poster},
  title = {Colorimetric and {{Positional Application}} to {{Breakdown LAMP Evaluation}} ({{CAPABLE}})},
